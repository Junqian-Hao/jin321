<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jin321.ms.dao.DealerLoginMapper">
    <resultMap id="LoginResultMap" type="DealerLogin">
        <result column="dusername" property="dusername" jdbcType="VARCHAR"/>
        <result column="dpassword" property="dpassword" jdbcType="VARCHAR"/>
    </resultMap>
    <select id="DealerLogin" resultMap="LoginResultMap">
        SELECT dusername,dpassword
        FROM Dealer
        WHERE dusername = #{dusername}
    </select>
</mapper>